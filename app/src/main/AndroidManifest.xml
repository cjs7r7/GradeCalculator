<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.umkc.connor.gradecalculator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/calculator"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AboutPage"
            android:label="@string/title_activity_about_page"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Assignments"
            android:label="@string/title_activity_assignments"
            android:parentActivityName=".GradeScaleSaved"
            android:theme="@style/AppTheme">
            />
        </activity>
        <activity
            android:name=".GradeScaleSaved"
            android:label="@string/title_activity_grade_scale_saved"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Continue"
            android:label="@string/title_activity_continue"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="edu.umkc.connor.gradecalculator.MainActivity" />
        </activity>
        <activity
            android:name=".NewClass"
            android:label="@string/title_activity_new_class"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="edu.umkc.connor.gradecalculator.MainActivity" />
        </activity>
    </application>

</manifest>
